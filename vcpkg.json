{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "mywebserver",
  "version": "1.0.0",
  "description": "A high-performance C++ web server evolving into an enterprise-grade IM backend service",
  "homepage": "https://github.com/sususu5/MyWebserver",
  "license": "MIT",
  "supports": "linux | osx",
  "builtin-baseline": "2cf2bcc60add50f79b2c418487d9cd1b6c7c1fec",
  "dependencies": [
    {
      "name": "protobuf",
      "version>=": "3.21.12"
    }
  ],
  "features": {
    "client": {
      "description": "Client dependencies",
      "dependencies": [
        {
          "name": "ftxui",
          "version>=": "6.1.9"
        }
      ]
    },
    "server": {
      "description": "Server dependencies",
      "dependencies": [
        "sqlpp11",
        "sqlpp11-connector-mysql",
        "jwt-cpp",
        "nlohmann-json"
      ]
    }
  }
